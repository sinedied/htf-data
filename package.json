{
  "name": "htf-data",
  "version": "1.0.0",
  "description": "CLI tool to process Hadra Trance Festival database export into valid data for the app",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sinedied/htf-data.git"
  },
  "main": "index.js",
  "bin": {
    "htf": "./bin/htf"
  },
  "scripts": {
