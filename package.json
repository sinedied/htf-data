  "bugs": {
    "url": "https://github.com/sinedied/htf-data/issues"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "fbgraph": "^1.4.1",
    "franc": "^3.1.1",
    "fs-extra": "^4.0.1",
    "fuse.js": "^3.0.5",
    "html-entities": "^1.2.1",
    "iconv-lite": "^0.4.18",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "popsicle": "^9.1.0"
  },
  "devDependencies": {
    "eslint": "^4.4.1"
